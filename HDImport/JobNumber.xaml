<Window x:Class="HDImport.JobNumber"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="JobNumber" Height="135" Width="300">
    <Grid Height="102" VerticalAlignment="Top">
        <Label Content="Job #" HorizontalAlignment="Left" Margin="10,10,0,65" Width="40"/>
        <TextBox Name="jobNumber" HorizontalAlignment="Left" Height="27" Margin="55,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="196"/>
        <Button Content="Save" Click="saveClick" HorizontalAlignment="Left" Height="33" Margin="10,59,0,0" VerticalAlignment="Top" Width="92"/>
        <Button Content="Cancel" Click="cancelClick" HorizontalAlignment="Left" Height="33" Margin="190,59,0,0" VerticalAlignment="Top" Width="92"/>
        <Label Content="Example: FF 12345" HorizontalAlignment="Left" Height="22" Margin="55,36,0,0" VerticalAlignment="Top" Width="135" FontSize="10"/>
    </Grid>
</Window>
